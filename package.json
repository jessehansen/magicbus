{
  "name": "magicbus",
  "version": "2.3.4",
  "description": "A message bus framework using RabbitMQ.",
  "main": "lib/index.js",
  "scripts": {
    "ci": "npm-run-all lint ci:*",
    "ci:test": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- -R ${MOCHA_REPORTER:-spec} --recursive test && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "postci:test": "istanbul check-coverage",
    "test": "npm-run-all test:*",
    "test:integration": "mocha --recursive test/**/*.integration-tests.js test/*.integration-tests.js",
    "test:unit": "mocha --recursive test/**/*.tests.js test/*.tests.js",
    "test:coverage": "istanbul cover node_modules/mocha/bin/_mocha -- -R ${MOCHA_REPORTER:-spec} --recursive test",
    "test:unit:coverage": "istanbul cover node_modules/mocha/bin/_mocha -- -R ${MOCHA_REPORTER:-spec} --recursive test/*.tests.js test/**/*.tests.js",
    "lint": "esw --quiet",
    "docs": "node_modules/.bin/jsdoc -c ./jsdoc.conf.json",
    "prepublish": "npm run docs",
    "preversion": "./scripts/check_branch.sh",
    "postversion": "git push origin master --tags"
  },
  "engines": {
    "node": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twindagger/magicbus.git"
  },
  "author": "Jesse Hansen <twindagger@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/twindagger/magicbus/issues"
  },
  "homepage": "https://github.com/twindagger/magicbus#readme",
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chalk": "^2.3.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.13.1",
    "eslint-watch": "^3.1.3",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.5.5",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "npm-run-all": "^4.1.2",
    "sinon": "^4.1.3",
    "sinon-chai": "^2.14.0",
    "winston": "^2.4.0"
  },
  "dependencies": {
    "amqplib": "^0.5.2",
    "assert-plus": "^1.0.0",
    "bluebird": "^3.5.1",
    "lodash": "^4.17.4",
    "machina": "^2.0.0",
    "monologue.js": "^0.3.5",
    "postal": "^2.0.5",
    "shortid": "^2.2.8"
  }
}
